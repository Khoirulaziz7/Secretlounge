# Token bot dari @BotFather
BOT_TOKEN=your_bot_token_here

# URL Database MySQL (gunakan PlanetScale, Railway, atau Supabase)
DATABASE_URL=mysql://user:password@host:port/database

# URL webhook untuk Vercel
WEBHOOK_URL=https://your-app.vercel.app/api/webhook

# ID Telegram owner bot (dapatkan dari @userinfobot)
OWNER_ID=123456789

# Username channel yang wajib di-join (tanpa @)
REQUIRED_CHANNEL=your_channel

# Salt untuk enkripsi ID (generate dengan: openssl rand -hex 16)
SECRET_SALT=your_secret_salt_here